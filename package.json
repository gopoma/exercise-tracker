{
  "name": "exercise-tracker",
  "version": "1.0.0",
  "description": "",
  "main": "./dist/src/server.js",
  "scripts": {
    "start": "dotenv -e .env.production -- npx cross-env NODE_ENV=production node ./dist/src/server.js",
    "dev": "dotenv -e .env.development -- npx cross-env NODE_ENV=development npx ts-node-dev ./src/server.ts",
    "build": "tsc",
    "migrate:mysql:dev": "dotenv -e .env.development -- npx prisma migrate dev",
    "migrate:mysql:test": "dotenv -e .env.test -- npx prisma migrate deploy",
    "test": "dotenv -e .env.test -- npx cross-env NODE_ENV=test npx ts-mocha ./test/**/*.test.ts"
  },
  "keywords": [],
  "author": "gopoma",
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.3.4",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.13",
    "@types/expect": "^24.3.0",
    "@types/express": "^4.17.17",
    "@types/mocha": "^10.0.1",
    "@types/morgan": "^1.9.4",
    "@types/node": "^18.14.6",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "chai": "^4.3.7",
    "eslint": "^8.35.0",
    "mocha": "^10.2.0",
    "morgan": "^1.10.0",
    "prisma": "^4.11.0",
    "supertest": "^6.3.3",
    "ts-mocha": "^10.0.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@prisma/client": "^4.11.0",
    "@sinclair/typebox": "^0.25.24",
    "ajv": "^8.12.0",
    "ajv-errors": "^3.0.0",
    "ajv-formats": "^2.1.1",
    "ajv-keywords": "^5.1.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^7.0.0",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "helmet": "^6.0.1",
    "http-status": "^1.6.2"
  }
}
